# hide docker's loopback devices from udisks, and thus from user desktops
SUBSYSTEM=="block", ENV***REMOVED***DM_NAME***REMOVED***=="docker-*", ENV***REMOVED***UDISKS_PRESENTATION_HIDE***REMOVED***="1", ENV***REMOVED***UDISKS_IGNORE***REMOVED***="1"
SUBSYSTEM=="block", DEVPATH=="/devices/virtual/block/loop*", ATTR***REMOVED***loop/backing_file***REMOVED***=="/var/lib/docker/*", ENV***REMOVED***UDISKS_PRESENTATION_HIDE***REMOVED***="1", ENV***REMOVED***UDISKS_IGNORE***REMOVED***="1"
